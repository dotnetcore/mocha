{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Metadata": {
    "Storage": {
      "Provider": "LiteDB",
      "LiteDB": {
        "DatabasePath": "../mocha_litedb"
      },
      "EF": "server=local-mysql.orb.local;userid=root;password=root;database=mocha;"
    }
  },
  "Trace": {
    "Storage": {
      "EF": "server=local-mysql.orb.local;userid=root;password=root;database=mocha;"
    }
  },
  "Metrics": {
    "Storage": {
      "Provider": "LiteDB",
      "LiteDB": {
        "DatabasePath": "../mocha_litedb"
      },
      "InfluxDB": {
        "Url": "http://local-influxdb.orb.local",
        "Token": "mocha_influxdb_token",
        "Org": "mocha_org",
        "Bucket": "mocha_metrics"
      }
    }
  }
}

